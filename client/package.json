  },
  "dependencies": {
    "@types/redux-actions": "^2.6.5",
    "antd": "^5.16.4",
    "antd-style": "^3.6.2",
    "axios": "^1.6.8",
    "next": "^14.2.2",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "redux-actions": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
