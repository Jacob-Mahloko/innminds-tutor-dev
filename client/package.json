{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-start": "npm run build && npm run start",
    "lint": "next lint",
    "test": "jest --coverage",
    "test-latest": "jest --onlyChanged",
    "format": "prettier --write . && git add -A .",
